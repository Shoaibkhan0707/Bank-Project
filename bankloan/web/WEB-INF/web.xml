<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>formsubmitter</servlet-name>
        <servlet-class>formsubmitter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>formsubmitter1</servlet-name>
        <servlet-class>newpackage.formsubmitter1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mportal</servlet-name>
        <servlet-class>newpackage.mportal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>applicantdetails</servlet-name>
        <servlet-class>newpackage.applicantdetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatedb</servlet-name>
        <servlet-class>newpackage.updatedb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginprovider</servlet-name>
        <servlet-class>newpackage.loginprovider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>welcome</servlet-name>
        <jsp-file>welcome.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>nnjsp</servlet-name>
        <jsp-file>nnjsp.jsp</jsp-file>
        <init-param>
            <param-name>country</param-name>
            <param-value>india</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>formsubmitter</servlet-name>
        <url-pattern>/formsubmitter</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/errorpage.html</location>
    </error-page>
    <servlet-mapping>
        <servlet-name>formsubmitter1</servlet-name>
        <url-pattern>/formsubmitter1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mportal</servlet-name>
        <url-pattern>/mportal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>applicantdetails</servlet-name>
        <url-pattern>/applicantdetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatedb</servlet-name>
        <url-pattern>/updatedb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginprovider</servlet-name>
        <url-pattern>/loginprovider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nnjsp</servlet-name>
        <url-pattern>/nnjsp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
        <param-name> url</param-name>
        <param-value>jdbc:mysql//localhost:3306/bank_loan?useSSL=false</param-value>
    </context-param>
    <context-param>
        <param-name>username</param-name>
        <param-value>shoaib</param-value>
    </context-param>
    <context-param>
        <param-name>password</param-name>
        <param-value>admin@123</param-value>
    </context-param>
     <context-param>
            <param-name>country</param-name>
            <param-value>india</param-value>
        </context-param>
</web-app>
